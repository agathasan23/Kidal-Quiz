<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#21015E"
    tools:context=".papanscore.PapanScoreActivity">

    <ImageView
        android:id="@+id/sqr"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/background_soal"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="60dp"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginBottom="100dp"/>

    <TextView
        android:id="@+id/nama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textColor="#5C5050"
        android:textSize="30dp"
        app:layout_constraintTop_toTopOf="@+id/sqr"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/font"/>

    <TextView
        android:id="@+id/your"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="YOUR SCORE"
        android:textColor="#5C5050"
        android:textSize="36dp"
        app:layout_constraintTop_toTopOf="@+id/sqr"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/font"/>

    <TextView
        android:id="@+id/score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="50dp"
        android:textColor="#000000"
        app:layout_constraintTop_toTopOf="@+id/sqr"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/font"/>

    <ImageView
        android:id="@+id/imgpiala"
        android:layout_width="wrap_content"
        android:layout_height="500dp"
        android:src="@drawable/piala"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="170dp"/>

    <TextView
        android:id="@+id/button"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:text="Lanjut>>>   "
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/rangking"
        android:textAlignment="textEnd"
        android:background="#000000"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:padding="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="0dp"
        android:fontFamily="@font/font"/>

    <TextView
        android:id="@+id/rangking"
        android:layout_width="0dp"
        android:layout_height="70dp"
        android:text="Lihat Rangking"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/button"
        android:textAlignment="textEnd"
        android:background="#000000"
        android:foreground="?attr/selectableItemBackgroundBorderless"
        android:padding="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="0dp"
        android:fontFamily="@font/font"/>

    <ImageView
        android:id="@+id/star"
